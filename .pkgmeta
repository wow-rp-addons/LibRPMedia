---
package-as: LibRPMedia
enable-nolib-creation: no

license-output: LICENSE

externals:
  Libs/LibStub: https://repos.wowace.com/wow/libstub/trunk

ignore:
  - Libs/*/*.rockspec
  - Libs/*/*.toc
  - Libs/*/changelog.*
  - Libs/*/CHANGELOG.*
  - Libs/*/docs
  - Libs/*/examples
  - Libs/*/README.*
  - Libs/*/tests
  - Exporter
  - Makefile
  - '*.rockspec'
